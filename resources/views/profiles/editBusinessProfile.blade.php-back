		if($("select[name='frm_servicetype']").val() == "") {
			msg += '<br>Choose Service Type';
			returnVal = false;
		}
		if($("select[name='frm_programtype']").val() == "") {
			msg += '<br>Choose Program Type';
			returnVal = false;
		}		
		if($("select[name='frm_agerange']").val() == "") {
			msg += '<br>Select Age Range';
			returnVal = false;
		}
		if($("select[name='frm_programfor']").val() == "") {
			msg += '<br>Select Program';
			returnVal = false;
		}		
		if($("select[name='frm_numberofpeople']").val() == "") {
			msg += '<br>Select Number of People';
			returnVal = false;
		}

		if($("select[name='frm_experience_level']").val() == "") {
			msg += '<br>Select Experience Level';
			returnVal = false;
		}
		if($("select[name='frm_servicelocation']").val() == "") {
			msg += '<br>Select Service Location';
			returnVal = false;
		}
		if($("select[name='frm_servicefocuses']").val() == "") {
			msg += '<br>Select Program Focuses';
			returnVal = false;
		}
		if($("select[name='frm_specialdeals']").val() == "") {
			msg += '<br>Select Special Deals';
			returnVal = false;
		}
		if($("select[name='frm_servicepriceoption']").val() == "") {
			msg += '<br>Select Service Price Options';
			returnVal = false;
		}
		if($("select[name='frm_serviceduration']").val() == "") {
			msg += '<br>Select Duration';
			returnVal = false;
		}
		if($("input[name='service_image']").val() == "") {
			msg += '<br>Select Profile pic';
			returnVal = false;
		}
		if($('#frm_tcdesc').val() == "") {
			msg += '<br>Enter Terms and Conditions';
			returnVal = false;
		}

		//-----------------------------------------------------------------------------------

                
		if(!returnVal) {
			showSystemMessages('#systemMessage_service', 'danger', msg);
			return false;
		}
		return true;		
	}

